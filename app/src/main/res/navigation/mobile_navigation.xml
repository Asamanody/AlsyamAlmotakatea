<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_profile"
    >
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.el3asas.regym.ui.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/profile_fragment" />
    <fragment
        android:id="@+id/navigation_home"
        android:name="com.el3asas.regym.ui.PlansFragment"
        android:label="@string/title_home"
        tools:layout="@layout/plans_frag" />

    <fragment
        android:id="@+id/navigation_slideshow"
        android:name="com.el3asas.regym.ui.SlideshowFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_slideshow"/>

</navigation>
